        <form role="form" id="frmSearchWarehouse">
            <input type="hidden" id="hdnSearchWarehouseId" value="" name="WarehouseFilter.Id" />

            <div class="card">
                <h5 class="card-header">
                    <span style="vertical-align: middle;" class="s-icon"><i class="ti-search"></i></span> &nbsp; Warehouse Search
                </h5>
                <div class="card-block p-t-0 p-b-0">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="">Warehouse Name</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Enter Warehouse name to search" name="WarehouseFilter.WarehouseName" maxlength="50" />
                                    <span class="input-group-btn">
                                        <button class="btn btn-default" type="button"> <i class="fa fa-search"></i> </button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        @*<div class="col-md-3">
                            <label for="">Company Mobile No.</label>
                            <div class="input-group">
                                <input type="text" class="form-control" id="" placeholder="Enter mobile no. to search" name="Company.MobileNo" maxlength="11">
                                <span class="input-group-addon">
                                    <i class="fa fa-mobile"></i>
                                </span>
                            </div>
                        </div>*@
                        <div class="col-md-3">
                            <div class="form-group">
                                <br />
                                <button type="button" id="btnSearchWarehouse" class="btn btn-purple btn-rounded label-left w-min-sm m-b-0-75 waves-effect waves-light"><span class="btn-label"><i class="ti-search"></i></span>Search</button>
                                <button type="reset" id="btnResetWarehouse" class="btn btn-warning btn-rounded label-left w-min-sm m-b-0-75 waves-effect waves-light"><span class="btn-label"><i clas s="ti-reload"></i></span>Reset</button>

                            </div>
                        </div>

                    </div>

                </div>
            </div>

        </form>

        <div class="card">

            <h5 class="card-header">
                <span style="vertical-align: middle;" class="s-icon"><i class="ti-wand"></i></span> &nbsp; Warehouse Listing
            </h5>

            <input type="hidden" id="hdnSearchWarehouseId" value="WarehouseId" name="WarehouseFilter.Id" />

            <div class="card-block p-a-0">
                <div class="row m-a-0">
                    <div class="col-md-12 p-a-0">
                        <div class="btn-group" style="float: right; padding: 2px 6px;">
                            <a style="border-top-left-radius: 20px; border-bottom-left-radius: 20px;" href="Index" class="btn btn-outline-info w-min-sm m-b-0-25 waves-effect waves-light">Create</a>
                            <button type="button" class="btn btn-outline-info w-min-sm m-b-0-25 waves-effect waves-light" id="btnEditWarehouse">Edit</button>
                            <button type="button" style="border-top-right-radius: 20px; border-bottom-right-radius: 20px;" class="btn btn-outline-info w-min-sm m-b-0-25 waves-effect waves-light" id="btnViewContactDetails">View Contact Details</button>
                        </div>
                        <table class="table m-b-0" id="tblWarehouse" data-current-page=""></table>
                    </div>
                </div>
            </div>
